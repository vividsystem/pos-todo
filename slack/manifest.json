{
    "display_information": {
        "name": "pos-todo",
        "description": "Printing stuff to my printer",
        "background_color": "#13265e",
        "long_description": "This bot enables you to print stuff to my receipt printer. You can use /pos-todo-print or click on the 3 dots on your message to print your message. It's my week8 project for #siege. You can find the source code on: https://github.com/vividsystem/pos-todo"
    },
    "features": {
        "bot_user": {
            "display_name": "pos-todo",
            "always_online": true
        },
        "shortcuts": [
            {
                "name": "Print to pos-todo",
                "type": "message",
                "callback_id": "pos_todo_print",
                "description": "Print your message to my printer"
            }
        ],
        "slash_commands": [
            {
                "command": "/pos-todo-print",
                "url": "https://pos-todo.vividsystem.hackclub.app/slack/command/print",
                "description": "Print to the printer!",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "channels:history",
                "channels:read",
                "chat:write",
                "chat:write.customize",
                "commands",
                "files:read",
                "files:write",
                "groups:history",
                "groups:read",
                "links:read",
                "links:write",
                "reactions:read",
                "reactions:write",
                "users:read",
                "im:write",
                "app_mentions:read"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "request_url": "https://pos-todo.vividsystem.hackclub.app/slack/events-endpoint",
            "bot_events": [
                "app_mention",
                "link_shared",
                "message.channels",
                "message.groups"
            ]
        },
        "interactivity": {
            "is_enabled": true,
            "request_url": "https://pos-todo.vividsystem.hackclub.app/slack/interactivity-endpoint"
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}
